# -----------------------------------------
# Example Environment Configuration
# -----------------------------------------
# Copy this file to `.env` in the project root.
# Fill in all required values before running the application.
# Do NOT commit your actual `.env` file with secrets to version control.


# Path to the directory where log files should be stored
LOG_DIRECTORY=

# Path to the directory where TSL (Trusted Service List) files will be downloaded or stored
TSL_DIRECTORY=

# Path to the pgAdmin volume for persisting configuration and user data
PGADMIN_DATA=

# Email address used to log in to pgAdmin
PGADMIN_EMAIL=

# Password for the pgAdmin user
PGADMIN_PASSWORD=

# PostgreSQL password for the main database user
POSTGRES_PASSWORD=

# PostgreSQL username
POSTGRES_USER=

# Name of the PostgreSQL database to be created/used
POSTGRES_DB=
